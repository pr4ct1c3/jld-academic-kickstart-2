	file = {Accepted Version:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\R4VGFFW2\\Eming et al. - 2014 - Wound repair and regeneration mechanisms, signali.pdf:application/pdf}
