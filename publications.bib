	number = {7},
	urldate = {2018-05-30},
	journal = {ACS Biomaterials Science \& Engineering},
	author = {Silveira, Camila P. and Apolinário, Letícia M. and Fávaro, Wagner J. and Paula, Amauri J. and Durán, Nelson},
