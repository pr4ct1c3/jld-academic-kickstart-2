@article{bachet_use_1999,
	title = {{THE} {USE} {OF} {BIOLOGICAL} {GLUE} {IN} {AORTIC} {SURGERY}},
	volume = {17},
	issn = {0733-8651},
	url = {http://www.sciencedirect.com/science/article/pii/S0733865105701144},
	doi = {10.1016/S0733-8651(05)70114-4},
	abstract = {Because vascular and cardic surgery exists, surgeons have faced technical difficulties mainly owing to the poor quality of the tissues, the unsafe sutures, and the possibility of catastrophic consequent hemorrhages. As a measure to reduce those deleterious difficulties, surgeons have imagined and developed a great number of technical artifacts and procedures. Among these, the idea of using adhesives and sealants appeared in the 1950s and rapidly resulted in numerous experimental studies These ideas stemmed from three main reasons: 1The potential rapidity of performing surgical repairs;2The possibility of immediately obtaining a tight and solid repair;3The possibility of reinforcing weak or torn tissues in order to perform safe sutures. To be used in a biological environment, an adhesive substance may comply to some conditions. These conditions and the required properties of biological surgical sealants were precisely defined and described in 1968 by Cooper and Falb in their report to the New York Academy of Sciences on surgical adhesives. A surgical adhesive must bond rapidly to the surrounding living tissues and keep its adhesive power. Although the main accepted mechanisms of adhesion based on physico-chemical aspects of molecular forces may apply, the nearly permanent presence of water or aqueous fluids in a biological environment may prevent the normal interplay of these forces at appropriate interfaces. The bonding forces, indeed, must not be reduced by the presence of water. Therefore, the substance must be insoluble or extremely slowly soluble in body fluids. It must, if possible, preserve the suppleness of the tissues. Lastly, it must not prove locally irritative or generally toxic, immediately or in the long term. Four different types of bonds are generally considered as being responsible for solid adhesion of sealants. They are the electro-static, covalent, metallic, or chemical (primary) bonds, and the residual or physical (secondary) bonds. Because of the high polarity of biological tissue surfaces, secondary forces might be an important factor in adhesion in surgical or biological use. But those forces are not strong enough to resist the hydrogen bonding of water (about 5 kcal/mol). The water, therefore, penetrates the interface and weakens the adhesion. Therefore, it is important to use substances that develope primary bonds with the biological tissues. A certain number of reactive groups have been identified in the tissues. Inside collagen, for instance, the amine, hydroxyl, and carboxyl groups represent about 25\% of the amino-acid components. A great number of functions are able to react with these groups. The selection of an adhesive system that can be used in surgery must take into account the presence of one of those groups. Additionally, many practical conditions must be taken into consideration. The adhesive agent must be easy to use, supplied in a small quantity, and easily storable. When applied, it must remain fluid but must lead to a rapid and efficacious adhesion. Because the application of heat and high pressure is not possible in a biological environment, the means of solidification of the adhesive agent are limited to rapid polymerisation of monomers, use of active prepolymers, absorption of a solvent, and solidification of a polymer emulsion under the action of a moderate pressure or a hydrogen ion concentration (pH) modification.20 A great variety of products thought to correspond to those conditions have been tested. Among those, polyacrylates, rubber latex, epoxy resins, formaldehyde resins, acetate and alcoholic polyvinyls, carboxylic polymers, etc., have demonstrated a more or less adhesive power. But none of them has demonstrated the necessary qualities to be used in a biological environment.},
	number = {4},
	journal = {Cardiology Clinics},
	author = {Bachet, Jean and Guilmet, Daniel},
	month = nov,
	year = {1999},
	pages = {779--796},
	file = {ScienceDirect Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\3SNNWFAV\\S0733865105701144.html:text/html}
}

@article{rousou_use_2013,
	title = {Use of fibrin sealants in cardiovascular surgery: a systematic review},
	volume = {28},
	issn = {1540-8191},
	shorttitle = {Use of fibrin sealants in cardiovascular surgery},
	doi = {10.1111/jocs.12099},
	abstract = {BACKGROUND: Fibrin sealants are used for hemostasis and tissue adherence.
AIM OF STUDY: This systematic review summarizes published clinical data for fibrin sealant use in cardiovascular surgery.
METHODS: A literature search for the following terms was conducted using PubMed and EMBASE: (TISSEEL or Tissucol or Beriplast P or Evicel or Quixil or Crosseal or Reliseal or Fibringluraas or Bolheal or Tachosil or Vivostat or Vitagel or Artiss or "fibrin glue" or "fibrin sealant" or "fibrin tissue adhesive") and (cardiac or cardiovascular or vascular or heart or coronary or surgery). Case reports and series were excluded; although reports of controlled trials were preferred, uncontrolled trial data were also considered.
RESULTS: Clinical trials and chart review analyses of fibrin sealants were identified and summarized. Although clinical trial data were available for other agents, the majority of published studies examined TISSEEL. Overall, TISSEEL and other fibrin sealants showed improvements over standard of care or control groups for a variety of predefined endpoints. Safety findings are also summarized.
CONCLUSIONS: Data from these studies showed that fibrin sealants were well tolerated and provided effective hemostasis in a range of cardiac and aortic surgeries.},
	language = {eng},
	number = {3},
	journal = {Journal of Cardiac Surgery},
	author = {Rousou, John A.},
	month = may,
	year = {2013},
	pmid = {23578196},
	keywords = {Humans, Fibrin Tissue Adhesive, Postoperative Complications, Cardiovascular Diseases, Double-Blind Method, Prospective Studies, Randomized Controlled Trials as Topic},
	pages = {238--247}
}

@article{boer_fibrin_2012,
	title = {Fibrin {Sealant} for {Prevention} of {Resection} {Surface}-related {Complications} {After} {Liver} {Resection}: {A} {Randomized} {Controlled} {Trial}},
	volume = {256},
	issn = {0003-4932},
	shorttitle = {Fibrin {Sealant} for {Prevention} of {Resection} {Surface}-related {Complications} {After} {Liver} {Resection}},
	url = {https://insights.ovid.com/pubmed?pmid=22791099},
	doi = {10.1097/SLA.0b013e3182602819},
	abstract = {To evaluate the efficacy of fibrin sealant in reducing resection surface-related complications in liver surgery. Bile leakage, bleeding, and abscess formation are major resection surface-related complications after liver resection. It is unclear whether application of fibrin sealant to the resection surface is effective in reducing these complications. In a multicenter, randomized trial in 310 noncirrhotic patients undergoing liver resection, we compared prophylactic application of fibrin sealant to the resection surface (156 patients) with no application of fibrin sealant (154 patients). In addition to clinical assessments, patients underwent protocolized computerized tomography (CT) scan 1 week postoperatively. Primary endpoint was a composite of postoperative resection surface-related complications (bile leakage, bleeding, or abscess), as adjudicated by a clinical-events committee that was unaware of the study-group assignments. Overall rate of resection surface-related complications was not different between the 2 groups: 24\% (38/156 patients) in the fibrin sealant group and 24\% (37/154 patients) in the control group. Bile leakage was detected in 14\% of patients in the fibrin sealant group and in 14\% of controls. CT scans showed a fluid collection at the resection surface 100 mL or more in 28\% of patients in the fibrin sealant group and in 26\% of controls (P = 0.800). The rate of reinterventions for resection surface-related complications (12\% vs 10\%; P = 0.492) and severity of complications did also not differ between the 2 groups. This randomized multicenter trial shows that prophylactic application of fibrin sealant at the resection surface after liver resections does not lead to a reduction in the incidence or severity of postoperative bile leakage or other resection surface-related complications (Controlled trial number, ISRCTN85205641).},
	language = {ENGLISH},
	number = {2},
	urldate = {2018-01-24},
	journal = {Annals of Surgery},
	author = {Boer, Marieke T. de and Klaase, Joost M. and Verhoef, Cornelis and Dam, Ronald M. van and Gulik, Thomas M. van and Molenaar, I. Quintus and Bosscha, Koop and Dejong, Cornelis H. c and Jagt, Eric J. Van der and Porte, Robert J.},
	month = aug,
	year = {2012},
	pmid = {00000658-201208000-00007},
	pages = {229--234},
	file = {Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\NVAF5TUR\\pubmed.html:text/html}
}

