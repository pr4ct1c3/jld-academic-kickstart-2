@article{liu_moldable_2017,
	title = {A {Moldable} {Nanocomposite} {Hydrogel} {Composed} of a {Mussel}-{Inspired} {Polymer} and a {Nanosilicate} as a {Fit}-to-{Shape} {Tissue} {Sealant}},
