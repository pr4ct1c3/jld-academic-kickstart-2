	journal = {ACS Nano},
	author = {He, Yanpu and Hong, Celestine and Li, Jiahe and Howard, MayLin T. and Li, Yingzhong and Turvey, Michelle E. and Uppu, Divakara S. S. M. and Martin, John R. and Zhang, Ketian and Irvine, Darrell J. and Hammond, Paula T.},
