	note = {Publisher: Taylor \& Francis
\_eprint: https://doi.org/10.1080/21645515.2018.1540827},
	keywords = {compliance, coverage, dosing, Mexico, Rotarix, RotaTeq, rotavirus},
	pages = {1251--1259},
	file = {Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\4HSJU2ZQ\\Luna-Casas et al. - 2019 - Vaccine coverage and compliance in Mexico with the.pdf:application/pdf;Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\GXSWZ9PD\\21645515.2018.html:text/html}
