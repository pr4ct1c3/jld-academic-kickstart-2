	pages = {369},
	file = {Park et al. - 2019 - Spray-Formed Layered Polymer Microneedles for Cont.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\NDGUQ4R6\\Park et al. - 2019 - Spray-Formed Layered Polymer Microneedles for Cont.pdf:application/pdf}
