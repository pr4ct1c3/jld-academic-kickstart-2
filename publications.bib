	keywords = {Animals, Disease Models, Animal, Swine, Burns/*therapy, *Hydrogels/chemistry/pharmacology, *Keratins/chemistry/pharmacology, *Silver Sulfadiazine/chemistry/pharmacology, Wound Healing/*drug effects},
