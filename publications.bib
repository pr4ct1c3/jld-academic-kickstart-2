	urldate = {2020-01-22},
	journal = {British Journal of Nursing},
	author = {McMullan, Christel and Blazeby, Jane and Donovan, Jenny L and Rooshenas, Leila and Elliott, Daisy and Mathers, Jonathan and {On behalf of the Bluebelle Study Group}},
