	author = {Yamaoka, Tetsuji and Takahashi, Yoshitake and Fujisato, Toshiya and Lee, Chan Woo and Tsuji, Takayuki and Ohta, Takayuki and Murakami, Akira and Kimura, Yoshiharu},
	month = mar,
