	file = {Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\EK8IVZ6I\\Kobayashi and Okano - 2019 - Design of Temperature-Responsive Polymer-Grafted S.pdf:application/pdf}
