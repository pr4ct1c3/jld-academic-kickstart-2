	volume = {243},
	issn = {0142-9612},
	url = {http://www.sciencedirect.com/science/article/pii/S0142961220301678},
	doi = {10.1016/j.biomaterials.2020.119921},
	abstract = {In vitro tissue engineered models are poised to have significant impact on disease modeling and preclinical drug development. Reliable methods to induce microvascular networks in such microphysiological systems are needed to improve the size and physiological function of these models. By systematically engineering several physical and biomolecular properties of the cellular microenvironment (including crosslinking density, polymer density, adhesion ligand concentration, and degradability), we establish design principles that describe how synthetic matrix properties influence vascular morphogenesis in modular and tunable hydrogels based on commercial 8-arm poly (ethylene glycol) (PEG8a) macromers. We apply these design principles to generate endothelial networks that exhibit consistent morphology throughout depths of hydrogel greater than 1 mm. These PEG8a-based hydrogels have relatively high volumetric swelling ratios ({\textgreater}1.5), which limits their utility in confined environments such as microfluidic devices. To overcome this limitation, we mitigate swelling by incorporating a highly functional PEG-grafted alpha-helical poly (propargyl-l-glutamate) (PPLGgPEG) macromer along with the canonical 8-arm PEG8a macromer in gel formation. This hydrogel platform supports enhanced endothelial morphogenesis in neutral-swelling environments. Finally, we incorporate PEG8a-PPLGgPEG gels into microfluidic devices and demonstrate improved diffusion kinetics and microvascular network formation in situ compared to PEG8a-based gels.},
	language = {en},
	urldate = {2020-06-03},
	journal = {Biomaterials},
	author = {Brown, Alexander and He, Hongkun and Trumper, Ella and Valdez, Jorge and Hammond, Paula and Griffith, Linda G.},
	month = jun,
	year = {2020},
	keywords = {Endothelial cells, Microvascular network, poly(ethylene glycol), poly(propargyl--glutamate), Swelling, Synthetic hydrogel},
	pages = {119921},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\HQB6PKFN\\Brown et al. - 2020 - Engineering PEG-based hydrogels to foster efficien.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\E7R4MGI7\\S0142961220301678.html:text/html}
}

@article{dunkelmann_engineered_2020,
	title = {Engineered triply orthogonal pyrrolysyl–{tRNA} synthetase/{tRNA} pairs enable the genetic encoding of three distinct non-canonical amino acids},
