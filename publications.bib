	pages = {S21--S28},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\XRCPN8E3\\Mankad M.B.B.S and odispoti - 2001 - The role of fibrin sealants in hemostasis.pdf:application/pdf;ScienceDirect Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\LFYRLKRD\\Mankad M.B.B.S and odispoti - 2001 - The role of fibrin sealants in hemostasis.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\9C7FBI6C\\S0002961001007735.html:text/html}
}

