	pages = {4323--32},
	annote = {Ong, Shin-YeuWu, JianMoochhala, Shabbir MTan, Mui-HongLu, JiaengNetherlandsBiomaterials. 2008 Nov;29(32):4323-32. doi: 10.1016/j.biomaterials.2008.07.034. Epub 2008 Aug 15.},
