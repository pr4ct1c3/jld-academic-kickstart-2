	keywords = {cell differentiation, fibrin, calcium phosphate(s), biomechanics, cell proliferation},
	pages = {377--385},
	file = {Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\9R8K7RE3\\Cui et al. - 2010 - The mechanical and biological properties of an inj.pdf:application/pdf;Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\AW8E8DZZ\\Cui et al. - 2010 - The mechanical and biological properties of an inj.pdf:application/pdf;Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\UN64CVX6\\abstract.html:text/html;Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\QKCM6GZU\\abstract.html:text/html}
