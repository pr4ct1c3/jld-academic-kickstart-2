	keywords = {Chitosan, Chemical Phenomena, Chemistry, Chitin, Enzyme Stability, Enzymes, Immobilized, Membranes, Artificial, Urease, immobilization},
	pages = {337--350},
	annote = {method for immobilization}
}

@article{zang_preparation_2014,
	title = {Preparation of {Magnetic} {Chitosan} {Nanoparticles} {As} {Support} for {Cellulase} {Immobilization}},
