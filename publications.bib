	issn = {0947-6539, 1521-3765},
	shorttitle = {Diffusive {Adhesives} for {Water}‐{Rich} {Materials}},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/chem.201900606},
	doi = {10.1002/chem.201900606},
	abstract = {It is notoriously difficult to adhere water-rich materials such as hydrogels and biological tissues. Existing adhesives usually suffer from weak and unadjustable adhesion strength partially because the contact between adhesives and substrates is largely restrained to the adhesive/substrate interface. Here, we approach this challenge by developing a class of diffusive adhesives (DAs) that can extend adhesion deep into the substrate to maximize adhesive/substrate contact. The DAs consist of hydrogel matrices and preloaded water-soluble monomers and crosslinkers, which will diffuse extensively into the water-rich substrates after adhesive/substrate contact. Then polymerization and crosslinking are triggered for the monomers to form a bridging network that interpenetrates with DA and substrate skeletons and topologically binds them together. Such kind of adhesion, in the absence of adhesive/substrate covalent bonding, is of high strength and toughness comparable to those of the best-performing natural and artificial adhesives. More importantly, we can precisely tune the adhesion strength on demand by manipulating the diffusion profile. It is envisioned that the DA family can be enriched from a large pool of hydrogel matrices and monomers and that they could be particularly useful in biological and medical applications.},
	language = {en},
	urldate = {2019-08-16},
	journal = {Chemistry – A European Journal},
	author = {Yang, Shenyu and Xu, Weiwei and Tu, Mei and Jiang, Lingxiang},
	month = may,
	year = {2019},
	pages = {chem.201900606},
	file = {Yang et al. - 2019 - Diffusive Adhesives for Water‐Rich Materials Stro.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\WL4KCNF7\\Yang et al. - 2019 - Diffusive Adhesives for Water‐Rich Materials Stro.pdf:application/pdf}
}

