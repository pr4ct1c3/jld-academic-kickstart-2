	pages = {561--569},
	file = {Pearton et al. - 2012 - Microneedle delivery of plasmid DNA to living huma.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\22GQXFJ2\\Pearton et al. - 2012 - Microneedle delivery of plasmid DNA to living huma.pdf:application/pdf}
}

