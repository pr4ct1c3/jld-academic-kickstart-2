@article{van_der_wal_biology_2007,
	title = {Biology of the peritoneum in normal homeostasis and after surgical trauma},
	volume = {9},
	issn = {1462-8910, 1463-1318},
	url = {http://doi.wiley.com/10.1111/j.1463-1318.2007.01345.x},
	doi = {10.1111/j.1463-1318.2007.01345.x},
	abstract = {The peritoneum is a serous membrane, which has a protective function for the contents of the abdominal cavity. It maintains homeostasis by allowing exchange of molecules and production of peritoneal ﬂuid, thus providing an environment in which intra-abdominal organs can function properly. When traumatized, whether by surgery or due to inﬂammatory processes, a series of responses come into action to regenerate the injured part of the peritoneum. The inﬂammatory reaction causes inﬂux of inﬂammatory cells but also activates resident mesothelial cells, ultimately leading to a ﬁbrinous exudate. Depending on the severity of the trauma this exudate is transient due to ﬁbrinolysis, or becomes more dense as a result of ﬁbroblasts persisting, leading to ﬁbrinous adhesions. A pivotal role is taken by the enzyme plasmin and its promotors and inhibitors; it is mainly the tissue-type plasminogen activator ⁄ plasminogen activator inhibitor ratio which determines the rate of ﬁbrinolysis and therefore the rate of adhesion formation. The rate of injury determines the rate and extent of the inﬂammatory response to that injury; in its turn the inﬂammatory reaction determines the extent of adhesion formation. One should realize this when performing intra-abdominal surgery, which is in fact operating inside the peritoneal organ.},
	language = {en},
	number = {s2},
	urldate = {2019-05-15},
	journal = {Colorectal Disease},
	author = {van der Wal, J. B. C. and Jeekel, J.},
	month = oct,
	year = {2007},
	pages = {9--13},
	file = {van der Wal and Jeekel - 2007 - Biology of the peritoneum in normal homeostasis an.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\VFF3J2QQ\\van der Wal and Jeekel - 2007 - Biology of the peritoneum in normal homeostasis an.pdf:application/pdf;van der Wal and Jeekel - 2007 - Biology of the peritoneum in normal homeostasis an.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\KT27F9ER\\van der Wal and Jeekel - 2007 - Biology of the peritoneum in normal homeostasis an.pdf:application/pdf}
}

@article{geim_microfabricated_2003,
