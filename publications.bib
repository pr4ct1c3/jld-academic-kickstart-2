	pages = {354--360},
	file = {PubMed Central Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\SJ363IKX\\Zumbansen et al. - 2009 - First siRNA library screening in hard-to-transfect.pdf:application/pdf}
