	month = jul,
	year = {2008},
	pages = {783--794},
	file = {Vandenbroucke et al. - 2008 - Prolonged gene silencing in hepatoma cells and pri.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\97ZYVCUD\\Vandenbroucke et al. - 2008 - Prolonged gene silencing in hepatoma cells and pri.pdf:application/pdf}
