	year = {2005},
	keywords = {Controlled release, antiproliferative drug, conjugate addition, glycosaminoglycan, polyethyleneglycol crosslinker, postsurgical adhesion prevention, rat uterine horn, thiol-reactive group},
	pages = {1275--1283},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\5LEPDJ66\\Liu et al. - 2005 - Crosslinked hyaluronan hydrogels containing mitomy.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\C2TNRYVR\\S0015028204032467.html:text/html}
