	month = dec,
	year = {2004},
	pages = {5875--5884},
	file = {Cohn and Lando - 2004 - Tailoring lactidecaprolactone co-oligomers as tis.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\UI68E3MA\\Cohn and Lando - 2004 - Tailoring lactidecaprolactone co-oligomers as tis.pdf:application/pdf;Cohn and Lando - 2004 - Tailoring lactidecaprolactone co-oligomers as tis.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\76LL5YEJ\\Cohn and Lando - 2004 - Tailoring lactidecaprolactone co-oligomers as tis.pdf:application/pdf}
