	abstract = {Since adhesive hydrogels showed wide applications ranging from wearable soft materials to medical sealants, more and more attention has been paid toward the exploration of novel adhesive hydrogels. However, the diﬃculty in removing the residue caused by the excessive adhesive strength and sluggish degradation or nondegradation behaviors of the adhesive has always been challenging. Inspired by the multiple complementary hydrogen bond interactions in DNA, the bioinspired nucleobase (A, T, and U) monomers were ﬁrst synthesized and used to tackify polyphosphoester hydrogels. The multiple hydrogen bonds and hydrophobic interactions between purine rings and pyrimidine functionalities endowed the hydrogels with excellent controllable adhesive properties. Besides this, it has been found that these nucleobase-tackiﬁed hydrogels could be easily peeled oﬀ without leaving any residue and could be totally degraded under alkaline conditions due to hydrolysis of phosphoester chains. At the same time, they also exhibited controllable biodegradation to diﬀerent extents under the diﬀerent pH conditions. The excellent adhesive performance, controllable biodegradation, and excellent biocompatibility showed by this nucleobase-tackiﬁed polyphosphoester adhesive hydrogel demonstrated its great potential in wound dressing, as a tissue sealant, and so on.},
	language = {en},
	number = {10},
	urldate = {2020-01-22},
	journal = {Biomacromolecules},
	author = {Wang, Wenliang and Liu, Sanrong and Chen, Binggang and Yan, Xinxin and Li, Shengran and Ma, Xiaojing and Yu, Xifei},
	month = oct,
	year = {2019},
	pages = {3672--3683},
	file = {Wang et al. - 2019 - DNA-Inspired Adhesive Hydrogels Based on the Biode.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\AZEDYC4Y\\Wang et al. - 2019 - DNA-Inspired Adhesive Hydrogels Based on the Biode.pdf:application/pdf;Wang et al. - 2019 - DNA-Inspired Adhesive Hydrogels Based on the Biode.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\M7LYKBEJ\\Wang et al. - 2019 - DNA-Inspired Adhesive Hydrogels Based on the Biode.pdf:application/pdf}
}

@article{anselmo_heat-stable_2019,
	title = {A heat-stable microparticle platform for oral micronutrient delivery},
