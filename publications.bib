	journal = {Journal of Biomaterials Applications},
	author = {Slezak, Paul and Klang, Andrea and Ferguson, James and Monforte, Xavier and Schmidt, Peter and Bauder, Barbara and Url, Angelika and Osuchowski, Marcin and Redl, Heinz and Spazierer, Daniel and Gulle, Heinz},
