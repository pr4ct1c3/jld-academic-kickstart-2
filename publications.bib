	file = {Oberli et al. - 2017 - Lipid Nanoparticle Assisted mRNA Delivery for Pote.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\ILNJ6W8A\\Oberli et al. - 2017 - Lipid Nanoparticle Assisted mRNA Delivery for Pote.pdf:application/pdf}
}

