	author = {Rahmanian-Schwarz, Afshin and Beiderwieden, Anna and Willkomm, Lina-Marie and Amr, Amro and Schaller, Hans-Eberhard and Lotter, Oliver},
	month = dec,
	year = {2011},
	keywords = {Suprathel, Biobrane, Biosynthetic wound dressing, Burn treatment, Cutometer, Skin substitute, Temporary dressing, Temporary wound covering},
	pages = {1343--1348},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\QI4KSNVL\\Rahmanian-Schwarz et al. - 2011 - A clinical evaluation of Biobrane® and Suprathel® .pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\2GL5IXBX\\S0305417911002105.html:text/html}
