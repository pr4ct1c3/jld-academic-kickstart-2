	language = {en},
	urldate = {2020-01-22},
	journal = {Biomaterials},
	author = {Koivusalo, Laura and Kauppila, Maija and Samanta, Sumanta and Parihar, Vijay Singh and Ilmarinen, Tanja and Miettinen, Susanna and Oommen, Oommen P. and Skottman, Heli},
