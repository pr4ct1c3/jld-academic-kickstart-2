@article{kusunoki_bioresorbable_2005,
	title = {Bioresorbable {Hyaluronate}-{Carboxymethylcellulose} {Membrane} ({Seprafilm}) in {Surgery} for {Rectal} {Carcinoma}: {A} {Prospective} {Randomized} {Clinical} {Trial}},
	volume = {35},
