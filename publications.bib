@article{brochhausen_intraperitoneal_2011,
	title = {Intraperitoneal adhesions—an ongoing challenge between biomedical engineering and the life sciences},
	volume = {98A},
	copyright = {Copyright © 2011 Wiley Periodicals, Inc.},
	issn = {1552-4965},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/jbm.a.33083},
	doi = {10.1002/jbm.a.33083},
	abstract = {Peritoneal adhesions remain a relevant clinical problem despite the currently available prophylactic barrier materials. So far, the physical separation of traumatized serosa areas using barriers represents the most important clinical strategy for adhesion prevention. However, the optimal material has not yet been found. Further optimization or pharmacological functionalization of these barriers could give an innovative input for peritoneal adhesion prevention. Therefore, a more complete understanding of pathogenesis is required. On the basis of the pathophysiology of adhesion formation the main barriers currently in clinical practice as well as new innovations are discussed in the present review. Physiologically, mesothelial cells play a decisive role in providing a frictionless gliding surface on the serosa. Adhesion formation results from a cascade of events and is regulated by a variety of cellular and humoral factors. The main clinically applied strategy for adhesion prevention is based on the use of liquid or solid adhesion barriers to separate physically any denuded tissue. Both animal and human trials have not yet been able to identify the optimal barrier to prevent adhesion formation in a sustainable way. Therefore, further developments are required for effective prevention of postoperative adhesion formation. To reach this goal the combination of structural modification and pharmacological functionalization of barrier materials should be addressed. Achieving this aim requires the interaction between basic research, materials science and clinical expertise. © 2011 Wiley Periodicals, Inc. J Biomed Mater Res Part A:, 2011.},
	language = {en},
	number = {1},
	urldate = {2020-03-25},
	journal = {Journal of Biomedical Materials Research Part A},
	author = {Brochhausen, Christoph and Schmitt, Volker H. and Rajab, Taufiek K. and Planck, Constanze N. E. and Krämer, Bernhard and Wallwiener, Markus and Hierlemann, Helmut and Kirkpatrick, C. James},
	year = {2011},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/jbm.a.33083},
	keywords = {barrier materials, mesothelial cells, peritoneal adhesions, peritoneal wound healing, postoperative adhesion formation},
	pages = {143--156},
	file = {Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\PG7JZ6P7\\Brochhausen et al. - 2011 - Intraperitoneal adhesions—an ongoing challenge bet.pdf:application/pdf;Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\7XUYFLWC\\jbm.a.html:text/html}
}

