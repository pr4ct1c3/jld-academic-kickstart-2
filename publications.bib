}

@article{ilyinskii_adjuvant-carrying_2014,
	series = {Vaccine {Technology} {IV}},
	title = {Adjuvant-carrying synthetic vaccine particles augment the immune response to encapsulated antigen and exhibit strong local immune activation without inducing systemic cytokine release},
