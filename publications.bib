	file = {PubMed Central Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\DYRKIDZT\\Vokrri et al. - 2015 - Experimental study of sutureless vascular anastomo.pdf:application/pdf}
