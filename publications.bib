@article{guex_plasma-functionalized_2014,
	title = {Plasma-functionalized electrospun matrix for biograft development and cardiac function stabilization},
	volume = {10},
	issn = {1742-7061},
	url = {http://www.sciencedirect.com/science/article/pii/S1742706114000099},
	doi = {10.1016/j.actbio.2014.01.006},
	abstract = {Cardiac tissue engineering approaches can deliver large numbers of cells to the damaged myocardium and have thus increasingly been considered as a possible curative treatment to counteract the high prevalence of progressive heart failure after myocardial infarction (MI). Optimal scaffold architecture and mechanical and chemical properties, as well as immune- and bio-compatibility, need to be addressed. We demonstrated that radio-frequency plasma surface functionalized electrospun poly(ɛ-caprolactone) (PCL) fibres provide a suitable matrix for bone-marrow-derived mesenchymal stem cell (MSC) cardiac implantation. Using a rat model of chronic MI, we showed that MSC-seeded plasma-coated PCL grafts stabilized cardiac function and attenuated dilatation. Significant relative decreases of 13\% of the ejection fraction (EF) and 15\% of the fractional shortening (FS) were observed in sham treated animals; respective decreases of 20\% and 25\% were measured 4 weeks after acellular patch implantation, whereas a steadied function was observed 4 weeks after MSC-patch implantation (relative decreases of 6\% for both EF and FS).},
	number = {7},
	urldate = {2015-03-27},
	journal = {Acta Biomaterialia},
	author = {Guex, A. G. and Frobert, A. and Valentin, J. and Fortunato, G. and Hegemann, D. and Cook, S. and Carrel, T. P. and Tevaearai, H. T. and Giraud, M. N.},
	month = jul,
	year = {2014},
	keywords = {electrospinning, Cardiac tissue engineering, In vivo functional evaluation, Plasma polymerization, Echocardiography: cell therapy},
	pages = {2996--3006},
	file = {1-s2.0-S1742706114000099-main.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\FVH83RHZ\\1-s2.0-S1742706114000099-main.pdf:application/pdf;ScienceDirect Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\HWHVRW6W\\Guex et al. - 2014 - Plasma-functionalized electrospun matrix for biogr.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\6DAXBKNR\\S1742706114000099.html:text/html}
}

@article{hoffman_airbrushed_2014,
	title = {Airbrushed {Composite} {Polymer} {Zr}-{ACP} {Nanofiber} {Scaffolds} with {Improved} {Cell} {Penetration} for {Bone} {Tissue} {Regeneration}},
	volume = {21},
	issn = {1937-3384},
	url = {http://online.liebertpub.com/doi/abs/10.1089/ten.tec.2014.0236},
	doi = {10.1089/ten.tec.2014.0236},
	abstract = {Electrospun polymer nanofibers have multiple applications in the tissue engineering field despite limited cell penetration within the scaffolds and slow synthesis rates. Airbrushing, a proposed alternative to traditional electrospinning, is a technique capable of synthesizing open structure nanofiber scaffolds at high rates. In this study, three biocompatible polymers—poly-D,L-lactic acid (P-DL-LA), polycaprolactone (PCL), and poly(methyl methacrylate) (PMMA), were airbrushed to form networks for bone tissue regeneration. All three polymers were loaded with up to 20\% (w/w) zirconium-modified amorphous calcium phosphate (Zr-ACP). A simple one-step mix and straightforward material deposition yielded open structure networks with well-distributed Zr-ACP. Cell penetration within the airbrushed scaffolds was found to be more than twice the cell penetration within conventional electrospun networks. The airbrushed polymer network supported cell growth and differentiation. Cells grown on the Zr-ACP in P-DL-LA fibers exhibited improved levels of osteocalcin protein with an increase in the Zr-ACP content by day 16. This airbrushing method promises to be a viable and attractive alternative to currently used electrospinning techniques in the formation of composite 3D nanofiber scaffolds for tissue engineering applications.},
	number = {3},
	urldate = {2015-03-27},
	journal = {Tissue Engineering Part C: Methods},
	author = {Hoffman, Kathleen and Skrtic, Drago and Sun, Jirun and Tutak, Wojtek},
	month = aug,
	year = {2014},
	pages = {284--291},
	file = {Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\Q6XZAISX\\Hoffman et al. - 2014 - Airbrushed Composite Polymer Zr-ACP Nanofiber Scaf.pdf:application/pdf;Hoffman et al. - 2014 - Airbrushed Composite Polymer Zr-ACP Nanofiber Scaf.pdf:C\:\\Users\\John\\Documents\\JLD\\Kofinas\\Solution Blow Spinning\\Hoffman et al. - 2014 - Airbrushed Composite Polymer Zr-ACP Nanofiber Scaf.pdf:application/pdf;Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\CRF2GWJR\\ten.tec.2014.html:text/html}
}

