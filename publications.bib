	pages = {745--747},
	file = {Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\PHUSEM5E\\John et al. - 2007 - Effective RNAi-mediated gene silencing without int.pdf:application/pdf}
}

