	journal = {Science Translational Medicine},
	author = {Mandal, Anasuya and Boopathy, Archana V. and Lam, Lionel K. W. and Moynihan, Kelly D. and Welch, Mary E. and Bennett, Nitasha R. and Turvey, Michelle E. and Thai, Nikki and Van, Jenny H. and Love, J. Christopher and Hammond, Paula T. and Irvine, Darrell J.},
	month = nov,
	year = {2018},
	pages = {eaar2227},
	file = {Mandal et al. - 2018 - Cell and fluid sampling microneedle patches for mo.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\4NWG5JTE\\Mandal et al. - 2018 - Cell and fluid sampling microneedle patches for mo.pdf:application/pdf}
