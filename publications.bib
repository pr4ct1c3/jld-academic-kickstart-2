	file = {Taboada et al. - 2020 - Overcoming the translational barriers of tissue ad.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\NDR9KC39\\Taboada et al. - 2020 - Overcoming the translational barriers of tissue ad.pdf:application/pdf}
}

