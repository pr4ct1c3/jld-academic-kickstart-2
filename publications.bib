	year = {2012},
	pmid = {22612730},
	keywords = {Animals, Collagen, Cyanoacrylates, Polyethylene Glycols, Humans, Thrombin, Tissue Adhesives, Cost Savings, Costs and Cost Analysis, Drug Approval, Drug Contamination, Equipment Contamination, Fibrinogen, Forecasting, Gelatin Sponge, Absorbable, Glutaral, Hemostatics, Infection Control, Inventions},
	pages = {2243--2255}
}

