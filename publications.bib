}

@article{alexander_nanomatrix_2018,
	title = {Nanomatrix {Coated} {Stent} {Enhances} {Endothelialization} but {Reduces} {Platelet}, {Smooth} {Muscle} {Cell}, and {Monocyte} {Adhesion} under {Physiologic} {Conditions}},
