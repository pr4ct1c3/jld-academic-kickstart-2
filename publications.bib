	journal = {Vaccine},
	author = {Martin, Julie E. and Louder, Mark K. and Holman, LaSonji A. and Gordon, Ingelise J. and Enama, Mary E. and Larkin, Brenda D. and Andrews, Charla A. and Vogel, Leatrice and Koup, Richard A. and Roederer, Mario and Bailer, Robert T. and Gomez, Phillip L. and Nason, Martha and Mascola, John R. and Nabel, Gary J. and Graham, Barney S.},
	month = nov,
	year = {2008},
	keywords = {Emerging infectious disease, T-cell vaccine, Vaccine clinical trial},
	pages = {6338--6343},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\YE8NSCRG\\Martin et al. - 2008 - A SARS DNA vaccine induces neutralizing antibody a.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\FKVNBX6I\\S0264410X08012279.html:text/html}
