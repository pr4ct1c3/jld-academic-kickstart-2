	pages = {769--775},
	file = {PubMed Central Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\A5EDAZIQ\\Brüggmann et al. - 2010 - Intra-abdominal Adhesions.pdf:application/pdf}
