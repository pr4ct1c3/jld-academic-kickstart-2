	file = {Kobayashi et al. - 2019 - On-site fabrication of Bi-layered adhesive mesench.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\4VHAZJ4Z\\Kobayashi et al. - 2019 - On-site fabrication of Bi-layered adhesive mesench.pdf:application/pdf}
