	month = nov,
	year = {2015},
	pages = {1915--1924},
	file = {Li et al. - 2015 - Biodistribution, excretion, and toxicity of mesopo.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\YZYB4R3F\\Li et al. - 2015 - Biodistribution, excretion, and toxicity of mesopo.pdf:application/pdf;si-tox-4-biodistribution-review.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\MAN749H5\\si-tox-4-biodistribution-review.pdf:application/pdf}
