	file = {Babaee et al. - 2019 - Temperature-responsive biometamaterials for gastro.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\BV6X9RFA\\Babaee et al. - 2019 - Temperature-responsive biometamaterials for gastro.pdf:application/pdf}
