	year = {1999},
	keywords = {biodegradable, tissue engineering, hydroxyapatite, PLGA, poly(caprolactone)},
	pages = {324--335},
	file = {Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\77VP7PKS\\Marra et al. - 1999 - In vitro analysis of biodegradable polymer blendh.pdf:application/pdf;Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\P5N7BTSI\\(SICI)1097-4636(19991205)473324AID-JBM63.0.html:text/html}
