	pages = {e488--e496},
	file = {Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\HMGJ8RQF\\Hellewell et al. - 2020 - Feasibility of controlling COVID-19 outbreaks by i.pdf:application/pdf}
}

