@article{logun_expanding_2019,
	title = {Expanding {Hydrophobically} {Modified} {Chitosan} {Foam} for {Internal} {Surgical} {Hemostasis}: {Safety} {Evaluation} in a {Murine} {Model}},
	volume = {239},
	issn = {00224804},
	shorttitle = {Expanding {Hydrophobically} {Modified} {Chitosan} {Foam} for {Internal} {Surgical} {Hemostasis}},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0022480419300629},
	doi = {10.1016/j.jss.2019.01.060},
	abstract = {Background: A novel injectable expanding foam based on hydrophobically modiﬁed chitosan (HM-CS) was developed to improve hemostasis during surgeries. HM-CS is an amphiphilic derivative of the natural biopolymer chitosan (CS); HM-CS has been shown to improve the natural hemostatic characteristics of CS, but its internal safety has not been systematically evaluated. The goal of this study was to compare the long-term in vivo safety of HM-CS relative to a commonly used ﬁbrin sealant (FS), TISSEEL (Baxter).
Methods: Sixty-four SpragueeDawley rats (275-325 g obtained from Charles River Laboratories) were randomly assigned to control (n ¼ 16) or experimental (n ¼ 48) groups. Samples of the test materials (HM-CS [n ¼ 16], CS [n ¼ 16], and FS [n ¼ 16]) applied to a nonlethal liver excision (0.4 Æ 0.3 g of the medial lobe) in rats were left inside the abdomen to degrade. Animals were observed daily for signs of morbidity and mortality. Surviving animals were sacriﬁced at 1 and 6 wk; the explanted injury sites were microscopically assessed.
Results: All animals (64/64) survived both the 1- and 6-wk time points without signs of morbidity. Histological examination showed a comparable pattern of degradation for the various test materials. FS remnants and signiﬁcant adhesions to neighboring tissues were observed at 6 wk. Residual CS and HM-CS were observed at the 6 wk with fatty deposits at the site of injury. Minimal adhesions were observed for CS and HM-CS.
Conclusions: The internal safety observed in the HM-CS test group after abdominal implantation indicates that injectable HM-CS expanding foam may be an appropriate internal use hemostatic candidate.},
	language = {en},
	urldate = {2019-08-16},
	journal = {Journal of Surgical Research},
	author = {Logun, Meghan T. and Dowling, Matthew B. and Raghavan, Srinivasa R. and Wallace, Mandy L. and Schmiedt, Chad and Stice, Steven and Karumbaiah, Lohitash},
	month = jul,
