	keywords = {Surgical adhesives, Types and applications, Biomimetic adhesives, Commercial products},
	pages = {1031--1050},
	file = {development-surgical-adhesive.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\AQJWNB35\\development-surgical-adhesive.pdf:application/pdf;Polymer-Surgical-Adhesives-Forecast.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\APZUSK6Z\\Polymer-Surgical-Adhesives-Forecast.pdf:application/pdf;ScienceDirect Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\AB4TMUD9\\Duarte et al. - 2012 - Surgical adhesives Systematic review of the main .pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\JXP3FPBM\\S0079670011001365.html:text/html}
