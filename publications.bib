	pages = {477--485},
	file = {Corrigan and Li - 2009 - Quantifying drug release from PLGA nanoparticulate.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\7WFWNSRR\\Corrigan and Li - 2009 - Quantifying drug release from PLGA nanoparticulate.pdf:application/pdf}
}

