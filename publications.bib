	note = {Publisher: Taylor \& Francis
\_eprint: https://doi.org/10.1080/21645515.2017.1389362},
	keywords = {vaccine, MERS coronavirus, Middle East Respiratory Syndrome, receptor-binding domain, spike protein},
	pages = {304--313},
	file = {Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\KTNZXAW4\\Cho et al. - 2018 - Development of Middle East Respiratory Syndrome Co.pdf:application/pdf;Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\TBUYIPNH\\21645515.2017.html:text/html}
