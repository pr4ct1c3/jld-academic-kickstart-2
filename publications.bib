	number = {4},
	urldate = {2020-05-24},
	journal = {The Lancet Infectious Diseases},
	author = {Parker, Lucy A and Rumunu, John and Jamet, Christine and Kenyi, Yona and Lino, Richard Laku and Wamala, Joseph F and Mpairwe, Allan M and Ciglenecki, Iza and Luquero, Francisco J and Azman, Andrew S and Cabrol, Jean-Clement},
