@article{kang_similar_2018,
	title = {Similar {Epitope} {Specificities} of {IgG} and {IgA} {Antibodies} {Elicited} by {Ad26} {Vector} {Prime}, {Env} {Protein} {Boost} {Immunizations} in {Rhesus} {Monkeys}},
