Publisher: Nature Publishing Group},
	pages = {343--355},
	file = {Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\GQ9LJAXC\\Xia et al. - 2020 - Inhibition of SARS-CoV-2 (previously 2019-nCoV) in.pdf:application/pdf}
}

