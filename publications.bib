	journal = {Science},
	author = {Hoegger, Mark J. and Fischer, Anthony J. and McMenimen, James D. and Ostedgaard, Lynda S. and Tucker, Alex J. and Awadalla, Maged A. and Moninger, Thomas O. and Michalski, Andrew S. and Hoffman, Eric A. and Zabner, Joseph and Stoltz, David A. and Welsh, Michael J.},
	month = aug,
	year = {2014},
	pmid = {25124441},
	pages = {818--822},
	file = {Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\24PGXERJ\\Hoegger et al. - 2014 - Impaired mucus detachment disrupts mucociliary tra.pdf:application/pdf;Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\DSMSSBP9\\818.html:text/html}
