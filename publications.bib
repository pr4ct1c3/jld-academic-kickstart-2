	author = {Chereddy, Kiran Kumar and Lopes, Alessandra and Koussoroplis, Salome and Payen, Valéry and Moia, Claudia and Zhu, Huijun and Sonveaux, Pierre and Carmeliet, Peter and des Rieux, Anne and Vandermeulen, Gaëlle and Préat, Véronique},
	month = nov,
	year = {2015},
	keywords = {Nanoparticles, Diabetic wound healing, Lactate, Poly(lactic--glycolic acid) (PLGA), VEGF},
	pages = {1975--1984},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\JI2RSJD9\\Chereddy et al. - 2015 - Combined effects of PLGA and vascular endothelial .pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\7GCU3ZPX\\S1549963415001495.html:text/html}
