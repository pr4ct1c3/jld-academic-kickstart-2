	file = {Kim et al. - 2015 - Toxicity of silica nanoparticles depends on size, .pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\Y3XGBVB5\\Kim et al. - 2015 - Toxicity of silica nanoparticles depends on size, .pdf:application/pdf;Si-Tox-1.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\ISU2W74H\\Si-Tox-1.pdf:application/pdf}
