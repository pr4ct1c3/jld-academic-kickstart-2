@article{fan_tough_2018,
	title = {Tough, {Swelling}-{Resistant}, {Self}-{Healing}, and {Adhesive} {Dual}-{Cross}-{Linked} {Hydrogels} {Based} on {Polymer}–{Tannic} {Acid} {Multiple} {Hydrogen} {Bonds}},
