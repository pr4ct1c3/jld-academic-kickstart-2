	number = {4},
	urldate = {2020-03-25},
	journal = {Fertility and Sterility},
	author = {Liu, Yanchun and Shu, Xiao Zheng and Prestwich, Glenn D.},
	month = apr,
	year = {2007},
	keywords = {polyethyleneglycol crosslinker, rat uterine horn, thiol-reactive group, adhesion models, hyaluronan, Rat cecum-abdominal wall, scar-free healing},
