	pages = {2456--2465},
	file = {Nasiri et al. - 2018 - Enhanced Mechanical and Adhesion Properties in Sus.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\BEBWAX5P\\Nasiri et al. - 2018 - Enhanced Mechanical and Adhesion Properties in Sus.pdf:application/pdf}
