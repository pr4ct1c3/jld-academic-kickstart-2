	journal = {American Journal of Obstetrics and Gynecology},
	author = {Bilic, Grozdana and Brubaker, Carrie and Messersmith, Phillip B. and Mallik, Ajit S. and Quinn, Thomas M. and Haller, Claudia and Done, Elisa and Gucciardo, Leonardo and Zeisberger, Steffen M. and Zimmermann, Roland and Deprest, Jan and Zisch, Andreas H.},
	month = jan,
	year = {2010},
	keywords = {tissue adhesive, DOPA, fetal membrane, fetoscopic access, fetoscopy, fibrin, iatrogenic PPROM, membrane sealing},
	pages = {85.e1--85.e9},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\MWEAIHGR\\Bilic et al. - 2010 - Injectable candidate sealants for fetal membrane r.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\3W3MCRQJ\\S0002937809008369.html:text/html}
