	pages = {796--803},
	annote = {Lao, Luciana LisaVenkatraman, Subbu SPeppas, Nicholas AengResearch Support, Non-U.S. Gov'tNetherlandsEur J Pharm Biopharm. 2008 Nov;70(3):796-803. doi: 10.1016/j.ejpb.2008.05.024. Epub 2008 Jun 6.},
	annote = {The following values have no corresponding Zotero field:auth-address: School of Materials Science and Engineering, Nanyang Technological University, Singapore, Singapore.edition: 2008/06/26accession-num: 18577449},
	file = {1-s2.0-S0939641108002105-main.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\TQAGAXKP\\1-s2.0-S0939641108002105-main.pdf:application/pdf;ScienceDirect Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\VYV7SDR4\\Lao et al. - 2008 - Modeling of drug release from biodegradable polyme.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\2892YQNF\\S0939641108002105.html:text/html}
