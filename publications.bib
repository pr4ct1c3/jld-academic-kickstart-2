	pages = {111--118},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\Z7N3MGCL\\Kamel - 2010 - Prevention of postoperative peritoneal adhesions.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\H58MKXZ8\\S0301211510000473.html:text/html}
