@article{debayle_zwitterionic_2019,
	title = {Zwitterionic polymer ligands: an ideal surface coating to totally suppress protein-nanoparticle corona formation?},
	volume = {219},
	issn = {01429612},
	shorttitle = {Zwitterionic polymer ligands},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0142961219304569},
	doi = {10.1016/j.biomaterials.2019.119357},
	abstract = {In the last few years, zwitterionic polymers have been developed as antifouling surface coatings. However, their ability to completely suppress protein adsorption at the surface of nanoparticles in complex biological media remains undemonstrated. Here we investigate the formation of hard (irreversible) and soft (reversible) protein corona around model nanoparticles (NPs) coated with sulfobetaine (SB), phosphorylcholine (PC) and carboxybetaine (CB) polymer ligands in model albumin solutions and in whole serum. We show for the ﬁrst time a complete absence of protein corona around SB-coated NPs, while PC- and CB-coated NPs undergo reversible adsorption or partial aggregation. These dramatic diﬀerences cannot be described by naïve hard/soft acid/base electrostatic interactions. Single NP tracking in the cytoplasm of live cells corroborate these in vitro observations. Finally, while modiﬁcation of SB polymers with additional charged groups lead to consequent protein adsorption, addition of small neutral targeting moieties preserves antifouling and enable eﬃcient intracellular targeting.},
	language = {en},
	urldate = {2019-10-03},
	journal = {Biomaterials},
	author = {Debayle, Manon and Balloul, Elie and Dembele, Fatimata and Xu, Xiangzhen and Hanafi, Mohamed and Ribot, Francois and Monzel, Cornelia and Coppey, Mathieu and Fragola, Alexandra and Dahan, Maxime and Pons, Thomas and Lequeux, Nicolas},
	month = oct,
	year = {2019},
	pages = {119357},
	file = {Debayle et al. - 2019 - Zwitterionic polymer ligands an ideal surface coa.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\9W38468N\\Debayle et al. - 2019 - Zwitterionic polymer ligands an ideal surface coa.pdf:application/pdf}
}

