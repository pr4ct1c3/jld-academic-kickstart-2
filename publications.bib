	author = {Ling, Ming-Hung and Chen, Mei-Chin},
	month = nov,
	year = {2013},
	pages = {8952--8961},
	file = {Ling and Chen - 2013 - Dissolving polymer microneedle patches for rapid a.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\E58MY343\\Ling and Chen - 2013 - Dissolving polymer microneedle patches for rapid a.pdf:application/pdf;Ling and Chen - 2013 - Dissolving polymer microneedle patches for rapid a.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\DK28MTMP\\Ling and Chen - 2013 - Dissolving polymer microneedle patches for rapid a.pdf:application/pdf}
