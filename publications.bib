	pages = {2226--2234},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\Q2HCYFS3\\Hayashi et al. - 2009 - The Stent-Eluting Drugs Sirolimus and Paclitaxel S.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\2HMVJTSG\\S0002944010607304.html:text/html}
