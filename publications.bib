@article{wilson_practicalities_2007,
	title = {Practicalities and costs of adhesions},
	volume = {9},
	issn = {1463-1318},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1463-1318.2007.01360.x},
	doi = {10.1111/j.1463-1318.2007.01360.x},
	abstract = {In spite of postoperative adhesions being common there appears to be a reluctance to use anti-adhesion products routinely. This article compares the incidence of adhesions with other conditions in order to identify the level of risk. The health economics surrounding adhesion-related disease are described. This combined information may be of help to convince health practitioners of the need to take a more active role in adhesion prevention. The SCAR project has identified the risk of adhesion-related disease. This is compared with published risks of other common clinical situations. An economic model first described by the author in 2002 has been revised with 2006 costs [1]. The SCAR data demonstrates a directly related risk of re-admission in certain groups of 9.4\% over 5 years [2]. The frequency of including this fact in the consenting process is low ({\textless}15\%) [3]. Legal precedent has identified a risk of {\textgreater}2\% warrants inclusion in the consent process; failure to do so could be considered negligent [4]. Use of an anti-adhesion product with a cost of 130 euros with an efficacy of 25\% in 1 year in the UK could save over 40 million euros over a 10-year period. Adhesion risk is frequent enough to include in consent. Failure to do this and avoidance of treatment, which may reduce adhesions will have major financial consequences on healthcare systems.},
	language = {en},
	number = {s2},
	urldate = {2020-03-25},
	journal = {Colorectal Disease},
	author = {Wilson, M. S.},
	year = {2007},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1463-1318.2007.01360.x},
	keywords = {adhesions, economics, Postoperative},
	pages = {60--65},
	file = {Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\M539L4SH\\Wilson - 2007 - Practicalities and costs of adhesions.pdf:application/pdf;Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\A9TQULAY\\j.1463-1318.2007.01360.html:text/html}
}

