	author = {Gaziano, John Michael and Concato, John and Brophy, Mary and Fiore, Louis and Pyarajan, Saiju and Breeling, James and Whitbourne, Stacey and Deen, Jennifer and Shannon, Colleen and Humphries, Donald and Guarino, Peter and Aslan, Mihaela and Anderson, Daniel and LaFleur, Rene and Hammond, Timothy and Schaa, Kendra and Moser, Jennifer and Huang, Grant and Muralidhar, Sumitra and Przygodzki, Ronald and O'Leary, Timothy J.},
