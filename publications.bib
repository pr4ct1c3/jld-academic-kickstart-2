	title = {Evasion of antibody neutralization in emerging severe acute respiratory syndrome coronaviruses},
	volume = {102},
	copyright = {Copyright © 2005, The National Academy of Sciences},
	issn = {0027-8424, 1091-6490},
	url = {https://www.pnas.org/content/102/3/797},
