	pages = {1--16},
	file = {Zhu et al. - 2018 - Bioadhesives for internal medical applications A .pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\6VXRCJLM\\Zhu et al. - 2018 - Bioadhesives for internal medical applications A .pdf:application/pdf}
}

