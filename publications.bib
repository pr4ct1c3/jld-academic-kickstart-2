	month = jan,
	year = {2016},
	keywords = {Biologic glue, Cyanoacrylate glue, Fibrin glue, Fibrin sealant, Groin hernia, Mesh fixation},
	pages = {239--249.e2},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\MUZ4V5CR\\Antoniou et al. - 2016 - Meta-analysis of randomized trials comparing nonpe.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\RJK6NALX\\S0002961015004134.html:text/html}
