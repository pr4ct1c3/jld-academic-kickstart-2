	abstract = {In this era of increasing antibiotic resistance, the use of alternative antimicrobials such as silver has become more widespread. Superior antimicrobial activity has been provided through fabrication of silver nanoparticles or nanosilver (NAg), which imparts cytotoxic actions distinct from those of bulk silver. In the wake of the recent discoveries of bacterial resistance to NAg and its rising incorporation in medical and consumer goods such as wound dressings and dietary supplements, we argue that there is an urgent need to monitor the prevalence and spread of NAg microbial resistance. In this Perspective, we describe how the use of NAg in commercially available products facilitates prolonged microorganism exposure to bioavailable silver, which underpins the development of resistance. Furthermore, we advocate for a judicial approach toward NAg use in order to preserve its eﬃcacy and to avoid environmental disruption.},
	language = {en},
	number = {4},
	urldate = {2018-05-31},
	journal = {ACS Nano},
	author = {Gunawan, Cindy and Marquis, Christopher P. and Amal, Rose and Sotiriou, Georgios A. and Rice, Scott A. and Harry, Elizabeth J.},
	month = apr,
	year = {2017},
	pages = {3438--3445},
	file = {Gunawan et al. - 2017 - Widespread and Indiscriminate Nanosilver Use Genu.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\JUN8JRZJ\\Gunawan et al. - 2017 - Widespread and Indiscriminate Nanosilver Use Genu.pdf:application/pdf;Gunawan et al. - 2017 - Widespread and Indiscriminate Nanosilver Use Genu.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\N4G2SFVS\\Gunawan et al. - 2017 - Widespread and Indiscriminate Nanosilver Use Genu.pdf:application/pdf}
