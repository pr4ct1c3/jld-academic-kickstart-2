	author = {Hayashi, Shin-ichiro and Yamamoto, Akitsugu and You, Fukka and Yamashita, Kentaro and Ikegame, Yuka and Tawada, Masahiro and Yoshimori, Tamotsu and Shimizu, Shigeomi and Nakashima, Shigeru},
	month = nov,
