}

@article{hong_layer-by-layer_2018,
	title = {Layer-by-layer {siRNA}/poly({L}-lysine) {Multilayers} on {Polydopamine}-coated {Surface} for {Efficient} {Cell} {Adhesion} and {Gene} {Silencing}},
