	file = {Svitkin et al. - 2017 - N1-methyl-pseudouridine in mRNA enhances translati.pdf:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\5M8R4JYF\\Svitkin et al. - 2017 - N1-methyl-pseudouridine in mRNA enhances translati.pdf:application/pdf}
}

