	note = {Publisher: Elsevier},
	file = {Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\665DHUX8\\Kucharski et al. - 2020 - Early dynamics of transmission and control of COVI.pdf:application/pdf;Full Text PDF:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\D8S96RZI\\Kucharski et al. - 2020 - Early dynamics of transmission and control of COVI.pdf:application/pdf;Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\JLB3BT7V\\fulltext.html:text/html;Snapshot:C\:\\Users\\John\\Dropbox\\JLD\\Kofinas\\Zotero\\storage\\IDJBTTWL\\fulltext.html:text/html}
